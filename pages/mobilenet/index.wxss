/* pages/mobilenet/index.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.4);
}

.camera {
  width: 100%;
  background-color: #fff;
  margin: 0 auto;
  z-index: 5;
}

.prediction-result {
  height: calc(100vh - 100vw); 
}
.cover-view{
  width: 100%;
  height: 100vh;
  font-size: 0;
  position: relative;
}
.mask{
  float: left;
  width: 20%;
  height: 100vh;
  background: rgba(0,0,0,.4);
}
.mid-mask{
  float: left;
  width: 60%;
  height: 100vh;
}
.mid-mask-top-bottom{
  width: 100%;
  height: 20vh;
  background: rgba(0,0,0,.4);
}
.scan-aera{
  width: 100%;
  height: 60vh;
  background: rgba(255, 255, 255, 0);
}

.logs{
  position: absolute;
  top: 15px;
  left: 10px;
  color: brown;
}
.graph_wall{
  position: absolute;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
}
.graph_wall .img {
  width: 100%;
  height: 100%;
  z-index: 5;
}
.graph_wall .mask{
  position: absolute;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  z-index: 10;
  background: rgba(0,0,0,.4);
}
.graph_wall .mask .content{
  position: absolute;
  bottom: 20%;
  left: 50%;
  z-index: 20;
  width: 70vw;
  text-align: center;
  color: #fff;
  transform: translateX(-50%);
  background: rgba(255, 255, 255, 0);

}