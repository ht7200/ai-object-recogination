/* pages/mobilenet/index.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.4);
}

.camera {
  width: 100%;
  background-color: #fff;
  margin: 0 auto;
  z-index: 5;
}

.prediction-result {
  height: calc(100vh - 100vw); 
}
.cover-view{
  width: 100%;
  height: 100vh;
  font-size: 0;
}
.mask{
  float: left;
  width: 20%;
  height: 100vh;
  background: rgba(0,0,0,.4);
}
.mid-mask{
  float: left;
  width: 60%;
  height: 100vh;
}
.mid-mask-top-bottom{
  width: 100%;
  height: 20vh;
  background: rgba(0,0,0,.4);
}
.scan-aera{
  width: 100%;
  height: 60vh;
  background: rgba(255, 255, 255, 0);
}

.logs{
  position: absolute;
  bottom: 35px;
  left: 10px;
  color: brown;
}